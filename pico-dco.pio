    jmp x-- lp0
    mov x, OSR  side 0
lp1:
jmp x-- lp1
pull_1:
    mov x, OSR
lp2:
jmp x-- lp2
pull_2:
    mov x, OSR
lp3:
jmp x-- lp3
pull_3:
    mov x, OSR
lp4:
jmp x-- lp4
